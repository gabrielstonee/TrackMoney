<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.trackmoney">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.trackmoney.activity.DespesasAcitivity"
            android:label="Despesas"
            android:theme="@style/Despesas">
        </activity>
        <activity android:name="com.example.trackmoney.activity.ReceitasAcitivity"
            android:label="Receitas"
            android:theme="@style/Receitas"/>
        <activity
            android:name="com.example.trackmoney.activity.PrincipalActivity"
            android:label="@string/title_activity_main2"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name="com.example.trackmoney.activity.CadastroActivity" />
        <activity android:name="com.example.trackmoney.activity.LoginActivity" />
        <activity
            android:name="com.example.trackmoney.activity.MainActivity"
            android:theme="@style/Theme.Intro">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>